<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#42A5F5"
    tools:context="com.example.mh.cb_sports_festival.ServiceCall">


    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:background="#FFFFFF"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="50dp"
            android:layout_height="56dp"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_weight="0.00"
            android:background="@android:color/holo_blue_light"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.962"
            app:layout_constraintLeft_toRightOf="@+id/imageButton5"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:popupTheme="@style/AppTheme.PopupOverlay" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:background="@android:color/background_light"
            android:onClick="onButtonHomeClicked"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/homebutton" />
    </android.support.constraint.ConstraintLayout>


    <ImageButton
            android:id="@+id/call1"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="80dp"
            android:onClick="call1Clicked"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@android:drawable/stat_sys_phone_call" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="210dp"
            android:layout_height="60dp"
            android:layout_marginEnd="45dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="45dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="80dp"
            android:text="전국체전 대표전화1
043-220-5211"
            android:textSize="20dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/call1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.512"
            android:textColor="@color/cardview_light_background"/>


        <TextView
            android:id="@+id/textView7"
            android:layout_width="210dp"
            android:layout_height="60dp"
            android:layout_marginEnd="45dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="45dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="44dp"
            android:text="전국체전 대표전화3
043-220-5213"
            android:textSize="20dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/call3"
            app:layout_constraintTop_toBottomOf="@+id/textView8"
            android:textColor="@color/cardview_light_background"/>


        <TextView
            android:id="@+id/textView11"
            android:layout_width="210dp"
            android:layout_height="60dp"
            android:layout_marginEnd="45dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="45dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="44dp"
            android:text="전국체전 대표전화4
043-220-5214"
            android:textSize="20dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/call4"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            android:textColor="@color/cardview_light_background"/>

        <TextView
            android:id="@+id/textView8"
            android:layout_width="210dp"
            android:layout_height="60dp"
            android:layout_marginEnd="45dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="45dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="44dp"
            android:text="전국체전 대표전화2
043-220-5212"
            android:textSize="20dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/call2"
            app:layout_constraintTop_toBottomOf="@+id/textView9"
            android:textColor="@color/cardview_light_background"/>


        <TextView
            android:id="@+id/textView10"
            android:layout_width="210dp"
            android:layout_height="60dp"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="44dp"
            android:text="충북나드리
043-220-4004"
            android:textSize="20dp"
            app:layout_constraintHorizontal_bias="0.3"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/call5"
            app:layout_constraintTop_toBottomOf="@+id/textView11"
            android:textColor="@color/cardview_light_background"/>


        <ImageButton
            android:id="@+id/call2"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="44dp"
            android:onClick="call2Clicked"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/call1"
            app:srcCompat="@android:drawable/stat_sys_phone_call_forward" />

        <ImageButton
            android:id="@+id/call3"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="44dp"
            android:onClick="call3Clicked"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/call2"
            app:srcCompat="@android:drawable/stat_sys_phone_call_forward" />

        <ImageButton
            android:id="@+id/call4"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="44dp"
            android:onClick="call4Clicked"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/call3"
            app:srcCompat="@android:drawable/stat_sys_phone_call_forward" />

        <ImageButton
            android:id="@+id/call5"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="44dp"
            android:onClick="call5Clicked"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/call4"
            app:srcCompat="@android:drawable/stat_sys_phone_call_forward" />



</android.support.constraint.ConstraintLayout>