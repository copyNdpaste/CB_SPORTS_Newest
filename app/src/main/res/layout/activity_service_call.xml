<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#42A5F5"
    tools:context="com.example.mh.cb_sports_festival.ServiceCall">


    <android.support.constraint.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="56dp"
        android:background="#FFFFFF"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="50dp"
            android:layout_height="56dp"
            android:layout_marginRight="0dp"
            android:layout_marginTop="0dp"
            android:layout_weight="0.00"
            android:background="@android:color/holo_blue_light"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:popupTheme="@style/AppTheme.PopupOverlay" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:background="@android:color/background_light"
            android:onClick="onButtonHomeClicked"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/homebutton" />
    </android.support.constraint.ConstraintLayout>


    <ImageButton
        android:id="@+id/call1"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="32dp"
        android:background="#42A5F5"
        android:onClick="call1Clicked"
        app:layout_constraintLeft_toRightOf="@+id/textView9"
        app:srcCompat="@mipmap/call"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="100dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="100dp"
        android:text="전국체전 대표전화1
043-220-5211"
        android:textColor="@color/cardview_light_background"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView7"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="44dp"
        android:text="전국체전 대표전화3
043-220-5213"
        android:textColor="@color/cardview_light_background"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />


    <TextView
        android:id="@+id/textView11"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="44dp"
        android:text="전국체전 대표전화4
043-220-5214"
        android:textColor="@color/cardview_light_background"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="44dp"
        android:text="전국체전 대표전화2
043-220-5212"
        android:textColor="@color/cardview_light_background"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />


    <TextView
        android:id="@+id/textView10"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="44dp"
        android:text="충북나드리
043-220-4004"
        android:textColor="@color/cardview_light_background"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />


    <ImageButton
        android:id="@+id/call2"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="8dp"
        android:background="#42A5F5"
        android:onClick="call2Clicked"
        app:layout_constraintLeft_toRightOf="@+id/textView8"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@mipmap/call"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/call1" />

    <ImageButton
        android:id="@+id/call3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="8dp"
        android:background="#42A5F5"
        android:onClick="call3Clicked"
        app:layout_constraintLeft_toRightOf="@+id/textView7"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@mipmap/call"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/call2" />

    <ImageButton
        android:id="@+id/call4"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="#42A5F5"
        android:onClick="call4Clicked"
        app:srcCompat="@mipmap/call"
        app:layout_constraintLeft_toRightOf="@+id/textView11"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/call3" />

    <ImageButton
        android:id="@+id/call5"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginLeft="32dp"
        android:layout_marginRight="8dp"
        android:background="#42A5F5"
        android:onClick="call5Clicked"
        app:layout_constraintHorizontal_bias="0.598"
        app:layout_constraintLeft_toRightOf="@+id/textView10"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@mipmap/call"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/call4" />


</android.support.constraint.ConstraintLayout>